{
  "name": "react-server-side-rendering",
  "version": "1.0.0",
  "description": "Server-side Rendering React",
  "main": "src/server.js",
  "engines": {
    "node": "8.11.1",
    "npm": "5.6.0"
  },
  "scripts": {
    "lint": "eslint . '**/.*.js'",
    "lint:fix": "npm run lint -- --fix",
    "start": "node build/server/index.js",
    "dev:server": "nodemon devServer.js",
    "dev:client": "npm run build:client -- --watch",
    "sass": "node-sass assets/sass/style.scss assets/style.css",
    "build:client": "webpack --config webpack.config.js",
    "build:server": "babel src -d build",
    "clean": "rimraf public/scripts build",
    "prebuild": "npm run clean",
    "build": "npm run build:client && npm run build:server",
    "format": "prettier-eslint --write --ignore **/node_modules/** \"**/*.{js,jsx}\"",
    "startSlides": "reveal-md slides.md -w --theme night"
  },
  "dependencies": {
    "express": "^4.16.2",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "reveal-md": "^1.1.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "node-sass": "^4.7.2",
    "nodemon": "^1.17.4",
    "prettier-eslint-cli": "^4.7.1",
    "rimraf": "^2.6.2",
    "webpack": "^4.1.0",
    "webpack-cli": "^2.0.10"
  }
}
